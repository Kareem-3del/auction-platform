{
  "name": "@minimal-kit/starter-next-ts",
  "author": "Minimals",
  "version": "7.0.0",
  "description": "Next Starter & TypeScript",
  "private": true,
  "scripts": {
    "dev": "next dev -p 8083 --turbopack",
    "start": "next start -p 8083",
    "build": "next build",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint:fix": "eslint --fix \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint:print": "npx eslint --print-config eslint.config.mjs > eslint-show-config.json",
    "fm:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx}\"",
    "fm:fix": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "fix:all": "npm run lint:fix && npm run fm:fix",
    "clean": "rm -rf node_modules .next out dist build",
    "re:dev": "yarn clean && yarn install && yarn dev",
    "re:build": "yarn clean && yarn install && yarn build",
    "re:build-npm": "npm run clean && npm install && npm run build",
    "tsc:dev": "yarn dev & yarn tsc:watch",
    "tsc:watch": "tsc --noEmit --watch",
    "tsc:print": "npx tsc --showConfig",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:reset": "prisma migrate reset",
    "db:seed": "tsx prisma/seed.ts",
    "db:studio": "prisma studio",
    "test": "jest",
    "postinstall": "prisma generate || echo 'Prisma generation skipped'",
    "vercel-build": "prisma generate && prisma db push && next build",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false"
  },
  "engines": {
    "node": ">=20"
  },
  "packageManager": "yarn@1.22.22",
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@emotion/cache": "^11.14.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@fontsource-variable/dm-sans": "^5.2.5",
    "@fontsource-variable/inter": "^5.2.5",
    "@fontsource-variable/nunito-sans": "^5.2.5",
    "@fontsource-variable/public-sans": "^5.2.5",
    "@fontsource/barlow": "^5.2.5",
    "@hookform/resolvers": "^4.1.3",
    "@iconify/react": "^5.2.0",
    "@mui/icons-material": "^7.3.1",
    "@mui/lab": "^7.0.0-beta.10",
    "@mui/material": "^6.1.0",
    "@mui/material-nextjs": "^7.0.0",
    "@mui/x-data-grid": "^7.28.2",
    "@mui/x-date-pickers": "^7.28.2",
    "@mui/x-tree-view": "^7.28.1",
    "@prisma/client": "^5.22.0",
    "argon2": "^0.31.2",
    "autosuggest-highlight": "^3.3.4",
    "axios": "^1.8.4",
    "bullmq": "^5.17.0",
    "date-fns": "^4.1.0",
    "dayjs": "^1.11.13",
    "es-toolkit": "^1.34.1",
    "framer-motion": "^12.6.1",
    "gsap": "^3.13.0",
    "ioredis": "^5.4.1",
    "jsonwebtoken": "^9.0.2",
    "minimal-shared": "^1.0.7",
    "nanoid": "^5.0.9",
    "next": "^15.2.4",
    "nprogress": "^0.2.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.55.0",
    "redis": "^4.7.0",
    "simplebar-react": "^3.3.0",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "stylis": "^4.3.6",
    "stylis-plugin-rtl": "^2.1.1",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.23.0",
    "@svgr/webpack": "^8.1.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@types/autosuggest-highlight": "^3.2.3",
    "@types/jest": "^29.5.13",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^22.13.14",
    "@types/nprogress": "^0.2.3",
    "@types/react": "^19.0.12",
    "@types/react-dom": "^19.0.4",
    "@types/stylis": "^4.2.7",
    "@types/supertest": "^6.0.2",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/parser": "^8.28.0",
    "eslint": "^9.23.0",
    "eslint-import-resolver-typescript": "^4.2.7",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-perfectionist": "^4.10.1",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "globals": "^16.0.0",
    "jest": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "prettier": "^3.5.3",
    "prisma": "^5.22.0",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "tsx": "^4.19.2",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.28.0"
  }
}
